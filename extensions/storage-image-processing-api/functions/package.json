{
  "name": "storage-image-processing-api-functions",
  "version": "0.0.1",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "EXPRESS_SERVER=true ../../../node_modules/.bin/nodemon",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md",
    "local:emulator": "cd ../../_emulator && firebase emulators:start -P demo-test import=./data",
    "test": "cd ../../_emulator && firebase emulators:exec jest -P demo-test",
    "test:watch": "concurrently \"npm run local:emulator\" \"jest --watch\"",
    "deploy": "firebase ext:install ../ --project=extensions-testing"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/lodash.get": "^4.4.6",
    "@types/sharp": "0.28.3",
    "a2a": "^0.2.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-type": "^16.5.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "helmet": "^4.6.0",
    "sharp": "0.30.7",
    "superstruct": "^0.15.2",
    "text-svg": "^1.0.3",
    "typescript": "^4.3.2"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "ts,json",
    "exec": "../../../node_modules/.bin/ts-node src/index.ts"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "firebase-functions-test": "^0.2.3",
    "jest": "^27.0.4"
  },
  "private": true
}
