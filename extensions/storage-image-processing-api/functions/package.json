{
  "name": "storage-image-processing-api-functions",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "EXPRESS_SERVER=true ../../../node_modules/.bin/nodemon",
    "emulator": "firebase ext:dev:emulators:start --test-params=test-params.env --project=extensions-testing",
    "generate-readme": "firebase ext:info .. --markdown > ../README.md",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "a2a": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-type": "^16.5.0",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "helmet": "^4.6.0",
    "sharp": "0.28.3",
    "superstruct": "^0.15.2",
    "text-svg": "^1.0.3"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "ts,json",
    "exec": "../../../node_modules/.bin/ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/lodash.get": "^4.4.6",
    "@types/sharp": "0.28.3",
    "axios": "^0.21.1",
    "firebase-functions-test": "^0.2.3",
    "typescript": "^4.3.2"
  },
  "private": true
}
