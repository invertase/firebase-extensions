{
  "extensions": {
    "sync-firestore-to-sheets": "../"
  },
  "emulators": {
    "hub": {
      "port": 4000
    },
    "auth": {
      "port": 9099
    },
    "pubsub": {
      "port": 8085
    },
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8080
    }
  },
  "functions": {
    "port": 5002,
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}